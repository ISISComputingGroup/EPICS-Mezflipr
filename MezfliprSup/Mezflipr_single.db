record(ai, "$(P)$(NAME):COMPENSATION") {
    field(INP, "@Mezflipr.proto getCompensation($(CODE)) $(PORT)")
    field(SCAN, "2 second")
    field(DTYP, "stream")
}

record(ao, "$(P)$(NAME):COMPENSATION:SP") {
    field(OUT, "@Mezflipr.proto setCompensation($(CODE)) $(PORT)")
    field(DTYP, "stream")
}

