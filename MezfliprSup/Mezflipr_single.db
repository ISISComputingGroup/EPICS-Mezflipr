record(ai, "$(P)$(NAME):COMPENSATION") {
    field(DESC, "$(NAME) Compensation")
    field(INP, "@Mezflipr.proto getCompensation($(CODE)) $(PORT)")
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(PREC, "2")
    info(interest, "HIGH")
    info(archive, "VAL")
    
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:COMPENSATION")
    field(SDIS, "$(P)DISABLE")
}

record(ao, "$(P)$(NAME):COMPENSATION:SP") {
    field(DESC, "$(NAME) Compensation SP")
    field(OUT, "@Mezflipr.proto setCompensation($(CODE)) $(PORT)")
    field(DTYP, "stream")
    field(PREC, "2")
    
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:COMPENSATION:SP")
    field(SDIS, "$(P)DISABLE")
}

